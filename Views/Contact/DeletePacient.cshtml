@model ContactModel

@{
    ViewData["title"] = "Deletar Paciente";
}

<div class="text-center">
    <h3>Realmente deseja apagar o paciente(@Model.Name) </h3>

</div>

<a class="btn btn-primary" role="button" asp-controller="contact" asp-action="Index" >Cancelar exclusao</a>
<a class="btn btn-danger" role="button"asp-controller="Contact" asp-action="Delete" asp-route-id="@Model.Id">Confirmar exclusao</a>